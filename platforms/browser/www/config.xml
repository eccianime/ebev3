<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ebetracking.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>EBE TRACKING</name>
    <description>
        El proyecto consiste en desarrollar una herramienta que reduzca el uso del papel y genere ahorros en
cuanto a uso y orden de la información.
    </description>
    <author email="ingjeanpaulrojas@gmail.com.com" href="http://ingjprojas.zz.com.ve">
        Ing. Jean Paul Iván Rojas Véliz
    </author>
    <content src="index.html" />
    <icon src="img/favicon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="fullscreen" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
